local LocalPlayer = game.Players.LocalPlayer
for _, v in LocalPlayer.Character:GetDescendants() do
    if v:IsA("BasePart") or v:IsA("Decal") then
    if v.Name ~= "HumanoidRootPart" then
        if v.Transparency == 1 then
            v.Transparency = 0
        else
            v.Transparency = 1
        end
    end
    end
end
